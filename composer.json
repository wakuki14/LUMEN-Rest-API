{
    "name": "LUMEN Rest API",
    "description": "A simple rest api for starting your project.",
    "author":"BUI TIEN HIEP<bthiep148@gmail.com>",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "facebook/graph-sdk": "^5.7",
        "firebase/php-jwt": "^5.0",
        "google/apiclient": "^2.4",
        "guzzlehttp/guzzle": "^6.4",
        "illuminate/mail": "^6.5",
        "laravel/lumen-framework": "^6.0",
        "mpociot/laravel-apidoc-generator": "^3.17"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
